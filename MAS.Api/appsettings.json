{
    "Serilog": {
      "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Seq", "Serilog.Enrichers.Environment", "Serilog.Enrichers.Thread", "Serilog.Enrichers.Process" ],
        "MinimumLevel": {
            "Default": "Information"
        },
        "Enrich": [ "WithMachineName", "WithProcessId", "WithThreadId", "WithEnvironmentUserName", "WithEnvironmentName" ],
        "WriteTo": [
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u3}] [Machine:{MachineName}] [Process:{ProcessId}] [Thread:{ThreadId}] [User:{EnvironmentUserName}] [Env:{EnvironmentName}] {Message:lj}{NewLine}{Exception}"
              }
            },
            {
              "Name": "File",
              "Args": {
                "path": "..\\MAS.Api\\Logs\\MASsengerLog-.txt",
                "rollingInterval": "Day",
                "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u3}] [Machine:{MachineName}] [Process:{ProcessId}] [Thread:{ThreadId}] [User:{EnvironmentUserName}] [Env:{EnvironmentName}] {Message:lj}{NewLine}{Exception}"
              }
            },
            {
              "Name": "Seq",
              "Args": { "serverUrl": "http://localhost:5341" }
            }
        ]
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "Data source=..\\MAS.Infrastracture\\Database\\MASsengerDB.db"
    },
    "Jwt": {
        "Key": "5676ebc92f428b36867190889b34bbf37a48b101a57695df1803cd72b342c3f4e386b10b1930",
        "ExpiryInMins": 5
    }
}
